# see https://github.com/rochus-keller/BUSY/

^run_nrc( 'res/all', 'all_.c', res ) # all.c would conflict with other Makefile declarations

let die * : Executable {
	.sources = [ 
		./main.c
		./dgui.c
		../casino/ddraw.c
	]
	.include_dirs += [ build_dir() ../casino ]
	.deps = [ ^static_all_lib res ]
	.configs += ^use_nappgui_static
}
